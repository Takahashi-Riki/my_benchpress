<!DOCTYPE html>
<html>
  <head>
    <title>MyBenchpress</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if flash[:notice] %>
      <%= flash[:notice] %>
    <% end %>
    <%= link_to "logout", logout_path, method: :delete %>
    <%= yield %>
  </body>
</html>
